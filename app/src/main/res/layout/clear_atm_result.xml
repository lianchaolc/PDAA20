<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dimen_480x"
    android:layout_height="@dimen/dimen_800y"
    android:background="#f6f6f6"
    android:orientation="vertical" >

    <LinearLayout
        android:gravity="center"
        android:background="@color/blue_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_123y" >

        <ImageView
            android:layout_marginRight="@dimen/dimen_15x"
            android:id="@+id/imageView1"
            android:layout_width="@dimen/dimen_49x"
            android:layout_height="@dimen/dimen_50y"
            android:src="@drawable/momey_img" />

        <TextView
            android:textSize="@dimen/text_size_40"
            android:textColor="@color/white"
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="清机加钞信息" />

        <ImageView
            android:layout_marginLeft="@dimen/dimen_70x"
            android:id="@+id/clearresult_back"
            android:layout_width="@dimen/dimen_64x"
            android:layout_height="@dimen/dimen_65y"
            android:src="@drawable/back_cirle" />

    </LinearLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TableRow
            android:id="@+id/TableRow02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_marginLeft="@dimen/dimen_20x"
                android:gravity="center"
                android:textColor="@color/red"
                android:id="@+id/TextView03"
                android:textSize="@dimen/text_size_25"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_40y"
                android:text="所在位置:"
                 />

            <TextView
                android:id="@+id/clear_result_address"
                android:textSize="@dimen/text_size_25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/back" />
        </TableRow>

        <TableRow
            android:id="@+id/TableRow01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_marginLeft="@dimen/dimen_20x"
                android:gravity="center"
                android:id="@+id/TextView01"
                android:textSize="@dimen/text_size_25"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_40y"
                android:text="机构编号:"
                android:textColor="@color/back" />

            <TextView
                android:id="@+id/clear_restlu_corpid"
                android:textSize="@dimen/text_size_25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/back" />
        </TableRow>

        <TableRow            
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_marginLeft="@dimen/dimen_20x"
                android:textSize="@dimen/text_size_25"
                android:textColor="@color/back"
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_40y"
                android:text="柜员编号:" />

            <TextView
                android:textSize="@dimen/text_size_25"
                android:textColor="@color/back"
                android:id="@+id/clear_result_moneyNum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="" />

        </TableRow>
    </TableLayout>

    <LinearLayout
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TableLayout
            android:layout_marginRight="@dimen/dimen_5x"
            android:layout_marginLeft="@dimen/dimen_5x"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TableRow
                android:id="@+id/TableRow03"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/gray" >

                <TextView
                    android:id="@+id/TextView06"
                    android:textSize="@dimen/text_size_25"
                    android:layout_width="@dimen/dimen_150x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_margin="1px"
                    android:background="@color/white"
                    android:gravity="center"
                    android:text="ATM编号"
                    android:textColor="@color/back" />

                <TextView
                    android:id="@+id/TextView05"
                    android:textSize="@dimen/text_size_25"
                    android:layout_width="@dimen/dimen_150x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_marginBottom="@dimen/dimen_1y"
                    android:layout_marginTop="@dimen/dimen_1y"
                    android:background="@color/white"
                    android:gravity="center"
                    android:text="钞箱编号"
                    android:textColor="@color/back" />

                <TextView
                    android:id="@+id/TextView07"
                    android:textSize="@dimen/text_size_25"
                    android:layout_width="@dimen/dimen_150x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_margin="1px"
                    android:background="@color/white"
                    android:gravity="center"
                    android:text="结果"
                    android:textColor="@color/back" />
            </TableRow>

            <TableRow
                android:background="@color/gray"
                android:id="@+id/tableRow6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

            </TableRow>
        </TableLayout>

        <ListView
            android:dividerHeight="1px"
            android:layout_marginRight="@dimen/dimen_14x"
            android:layout_marginLeft="@dimen/dimen_15x"
            android:id="@+id/listview_clearresult"
            android:layout_width="@dimen/dimen_455x"
            android:layout_height="@dimen/dimen_310y" >
        </ListView>

        <LinearLayout
            android:layout_marginTop="@dimen/dimen_30y"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_70y" >

            <Button
                android:textSize="@dimen/text_size_30"
                android:textColor="@color/white"
                android:background="@drawable/buttom_selector_bg"
                android:id="@+id/sure_clearresult"
                android:layout_width="@dimen/dimen_300x"
                android:layout_height="match_parent"
                android:text="确定" />

        </LinearLayout>

    </LinearLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
        </TableRow>

        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
        </TableRow>

        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
        </TableRow>
    </TableLayout>

</LinearLayout>