<?xml version="1.0" encoding="utf-8"?>
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:layout_gravity="center"
	    android:layout_marginLeft="@dimen/dimen_8x"
	    android:layout_marginRight="@dimen/dimen_8x"
	    android:layout_marginTop="@dimen/dimen_8y"
	    android:background="@color/white"
	    android:orientation="vertical">
	    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1y"
        android:background="#cccccc" >
         </View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:layout_gravity="center_horizontal"
        android:background="@color/white"
        android:orientation="horizontal" >

        <View
            android:layout_width="@dimen/dimen_1x"
            android:layout_height="match_parent"
            android:background="#cccccc" >
        </View>

        <TextView
            android:id="@+id/accountinfotion_listitem_tvinfo"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dimen_8x"
            android:layout_weight="1"
            android:gravity="center"
            android:text="22222"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_24" />


        <TextView
            android:id="@+id/accountinfotion_listitem_tvtype"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="2A"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_24" />

        <View
            android:layout_width="@dimen/dimen_1x"
            android:layout_height="match_parent"
            android:background="#cccccc" >
        </View>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1y"
        android:background="#cccccc" >
    </View>

	</LinearLayout>