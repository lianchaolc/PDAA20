<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_zhuangxiang_qingling"
    android:layout_width="@dimen/dimen_480x"
    android:layout_height="@dimen/dimen_800y"
    android:background="#ffffff"
    android:orientation="vertical" >

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_80y"
                android:background="@color/bule_main" >

                <ImageView
                    android:id="@+id/iv_zhuangxiang_qingling_back"
                    android:layout_width="@dimen/dimen_64x"
                    android:layout_height="@dimen/dimen_64y"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dimen_14x"
                    android:src="@drawable/back_change" />

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#00aeFF"
                android:orientation="vertical" >

                <fragment
                    android:id="@+id/fg_zhuangxiang_qingling_loginUserInfo"
                    android:name="com.ljsw.tjbankpda.qf.fragment.LoginUserInfoFragment"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_50y" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_316y"
                android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_38y"
        android:background="@color/bule_main" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/dimen_30x"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="@dimen/dimen_11x"
            android:src="@drawable/user_order" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_8x"
            android:layout_toRightOf="@+id/imageView1"
            android:text="装箱请领"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_18" />

    </RelativeLayout>

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_26y" >

                <TextView
                    android:id="@+id/TextView01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/dimen_38x"
                    android:text="现金："
                    android:textColor="@color/red"
                    android:textSize="@dimen/text_size_18" />

                <TextView
                    android:id="@+id/tv_qingfengrenwu_qinglingxiangqing_xianjing_total"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_toRightOf="@+id/TextView01"
                    android:text="1,250,000"
                    android:textSize="@dimen/text_size_16" />
            </RelativeLayout>

            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1y"
                android:layout_marginLeft="@dimen/dimen_15x"
                android:layout_marginRight="@dimen/dimen_15x"
                android:background="@drawable/gradient_white_gray" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dimen_30x"
                android:layout_marginRight="@dimen/dimen_30x" >

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="@dimen/dimen_0x"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="券别代码"
                    android:textColor="@color/bule_main" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="@dimen/dimen_0x"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="数量"
                    android:textColor="@color/bule_main" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_qingfenrenwu_qinglingxiangqing_xianjinInfo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dimen_30x"
                android:layout_marginRight="@dimen/dimen_30x"
                android:orientation="vertical" >

            </LinearLayout>

            <View
                android:id="@+id/View01"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1y"
                android:layout_marginLeft="@dimen/dimen_15x"
                android:layout_marginRight="@dimen/dimen_15x"
                android:background="@drawable/gradient_white_gray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_26y" >

                <TextView
                    android:id="@+id/tv_item_qinglingmingzi_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/dimen_38x"
                    android:text="重空凭证："
                    android:textColor="@color/red"
                    android:textSize="@dimen/text_size_18" />

                <TextView
                    android:id="@+id/tv_qingfengrenwu_qinglingxiangqing_zhongkong_total"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_toRightOf="@+id/tv_item_qinglingmingzi_type"
                    android:text="100"
                    android:textSize="@dimen/text_size_16" />

            </RelativeLayout>

            <View
                android:id="@+id/View02"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1y"
                android:layout_marginLeft="@dimen/dimen_15x"
                android:layout_marginRight="@dimen/dimen_15x"
                android:background="@drawable/gradient_white_gray" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dimen_30x"
                android:layout_marginRight="@dimen/dimen_30x" >

                <TextView
                    android:id="@+id/TextView03"
                    android:layout_width="@dimen/dimen_0x"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="凭证种类"
                    android:textColor="@color/bule_main" />

                <TextView
                    android:id="@+id/TextView04"
                    android:layout_width="@dimen/dimen_0x"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="数量"
                    android:textColor="@color/bule_main" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_qingfenrenwu_qinglingxiangqing_zhongkongInfo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >
            </LinearLayout>

            <View
                android:id="@+id/View03"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1y"
                android:layout_marginLeft="@dimen/dimen_15x"
                android:layout_marginRight="@dimen/dimen_15x"
                android:background="@drawable/gradient_white_gray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_26y" >

                <TextView
                    android:id="@+id/TextView05"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/dimen_38x"
                    android:text="抵质押品："
                    android:textColor="@color/red"
                    android:textSize="@dimen/text_size_18" />

                <TextView
                    android:id="@+id/tv_qingfengrenwu_qinglingxiangqing_sizhi_total"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_toRightOf="@+id/TextView05"
                    android:text="20"
                    android:textSize="@dimen/text_size_16" />
            </RelativeLayout>

            <Button
                android:id="@+id/btn_qingfengrenwu_qinglingxiangqing_zhuangxiang"
                android:layout_width="@dimen/dimen_150x"
                android:layout_height="@dimen/dimen_30y"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/dimen_22y"
                android:background="@drawable/buttom_selector_bg"
                android:text="装箱"
                android:textSize="@dimen/text_size_18" />

        </LinearLayout>
    </ScrollView>

</LinearLayout>

        </LinearLayout>
    </ScrollView>

</LinearLayout>