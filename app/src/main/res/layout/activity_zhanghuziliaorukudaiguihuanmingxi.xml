<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFEEEE"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80y"
        android:background="@color/blue_title"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_15x"
            android:src="@drawable/user_order" />

        <TextView
            android:id="@+id/textView1"
            style="@style/TextView_TitleStyle"
            android:text="待归还资料明细" />

        <Button
            android:id="@+id/ql_ruku_update"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_15x"
            android:layout_marginTop="@dimen/dimen_4y"
            android:background="@drawable/replace" />

        <ImageView
            android:id="@+id/ql_ruku_back"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:src="@drawable/back_cirle" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:layout_gravity="center"
        android:background="@color/white"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_60y"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/dimen_8x"
            android:layout_marginRight="@dimen/dimen_8x"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="操作人:"
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28"  />

            <TextView
                android:id="@+id/accountinfo_returnlist_oraperet"
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="白小白"
                android:textColor="@color/back"
                 android:textSize="@dimen/text_size_28"  />
        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/dimen_8x"
            android:layout_marginRight="@dimen/dimen_8x"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="操作人:"
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28"  />

            <TextView
                android:id="@+id/tvusername2"
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="李三样"
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28"  />
        </LinearLayout>
    </LinearLayout><LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dimen_10y"
        android:layout_marginTop="@dimen/dimen_10y"
        android:background="@color/white"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_80y"
            android:layout_gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dimen_8x"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="出库任务编号:"
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28" />

            <TextView
                android:id="@+id/accountinfo_returnlist_number"
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/dimen_8x"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text=""
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_45y"
            android:layout_gravity="center"
          
            android:orientation="horizontal" >
<TextView
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dimen_8x"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="账户资料袋数量:"
                android:textColor="@color/back"
                 android:textSize="@dimen/text_size_28" />

            <TextView
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/dimen_8x"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="4"
                android:textColor="@color/back"
                 android:textSize="@dimen/text_size_28"  />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_400y"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:layout_marginRight="@dimen/dimen_10x"
            android:layout_marginTop="@dimen/dimen_10y"
           
            android:orientation="vertical" >

            <ListView
                android:id="@+id/accountinfo_returnlist_listview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:divider="#00000000" >
            </ListView>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>