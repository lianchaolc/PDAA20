<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/homemenu_box"
    android:layout_width="@dimen/dimen_480x"
    android:layout_height="@dimen/dimen_800y"
    android:background="#ffffff"
    android:orientation="vertical"
    tools:context=".BaggingActivity" >
 <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80y"
        android:background="@color/blue_title"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_15x"
            android:src="@drawable/user_order" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:text="装箱操作"
            android:textColor="@color/white"
            android:textSize="@dimen/title_text_size" />

        <Button
            android:id="@+id/ql_ruku_update"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_90x"
            android:layout_marginTop="@dimen/dimen_4y"
            android:background="@drawable/replace" />

        <ImageView
            android:id="@+id/ql_ruku_back"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:src="@drawable/back_cirle" />
    </LinearLayout>
   <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_510y"
        android:layout_marginBottom="@dimen/dimen_4y"
        android:layout_marginLeft="@dimen/dimen_3x"
        android:layout_marginRight="@dimen/dimen_3x"
        android:layout_marginTop="@dimen/dimen_4y"
        android:background="@drawable/money_count"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_1x"
            android:layout_marginRight="@dimen/dimen_1x"
            android:layout_marginTop="@dimen/dimen_1y" >

            <Button
                android:id="@+id/btn_bc_cash"
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="@dimen/dimen_60y"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="现金"
                android:textSize="@dimen/text_size_35" />

            <View
                android:layout_width="@dimen/dimen_1x"
                android:layout_height="@dimen/dimen_60y"
                android:background="#000000" />

            <Button
                android:id="@+id/btn_bcimportnull"
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="@dimen/dimen_60y"
                android:layout_weight="1"
                android:background="@drawable/btn_bottom_gray"
                android:text="重空凭证"
                android:textSize="@dimen/text_size_35" />

            <View
                android:layout_width="@dimen/dimen_1x"
                android:layout_height="@dimen/dimen_60y"
                android:background="#000000" />

            <Button
                android:id="@+id/btn_ac_yuliu"
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="@dimen/dimen_60y"
                android:layout_weight="1"
                android:background="@drawable/btn_bottom_gray"
                android:text="预留"
                android:textSize="@dimen/text_size_35" />
        </LinearLayout>

        <FrameLayout
            android:id="@+id/fl_qingfen_ac_context"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="2dp" >
        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80y" >

        <Button
            android:id="@+id/btn_acmakecard"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_15x"
            android:layout_marginRight="@dimen/dimen_5x"
            android:layout_marginTop="@dimen/dimen_5y"
            android:layout_weight="1"
            android:background="@drawable/buttom_selector_bg"
            android:text="生成标签"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_35" />

        <Button
            android:id="@+id/btn_acreadcard"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_5x"
            android:layout_marginRight="@dimen/dimen_15x"
            android:layout_marginTop="@dimen/dimen_5y"
            android:layout_weight="1"
            android:background="@drawable/buttom_selector_bg"
            android:text="读取标签"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_35" />
    </LinearLayout>   
 
 
 
   
</LinearLayout>