<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dimen_480x"
    android:layout_height="@dimen/dimen_800y"
    android:background="#FFFFFF"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/blue_top"
        android:orientation="vertical" >

        <fragment 
            android:id="@+id/f3"
            android:name="com.fragment.pda.HeadUser_fragment"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_50y"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_100y"
        android:background="@color/blue_title"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="@dimen/dimen_72x"
                android:layout_height="match_parent"
                android:gravity="center_vertical|right"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="@dimen/dimen_40x"
                    android:layout_height="@dimen/dimen_40y"
                    android:src="@drawable/out_atm_box" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:orientation="vertical" >

                <TextView
                    android:layout_marginBottom="@dimen/dimen_20y"
                    android:id="@+id/title_noway"
                    android:textSize="@dimen/text_size_35"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_10x"
                    android:layout_marginTop="@dimen/dimen_27y"
                    android:text="出库操作" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/backnoway"
                android:layout_width="@dimen/dimen_70x"
                android:layout_height="@dimen/dimen_70y"
                android:src="@drawable/back_cirle" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_500y"
        android:orientation="vertical" >
    
     <LinearLayout
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:gravity="center_vertical"
         android:orientation="horizontal" >

         <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:orientation="vertical" >

             <LinearLayout
                 android:id="@+id/lbiz_name"
                 android:layout_width="match_parent"
                 android:layout_height="@dimen/dimen_50y"
                 android:layout_marginBottom="@dimen/dimen_10y"
                 android:gravity="center_vertical"
                 android:orientation="horizontal" >

                 <TextView
                     android:id="@+id/biznumber_noway"
                     android:textSize="@dimen/text_size_30"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_marginLeft="@dimen/dimen_10x"
                     android:layout_marginRight="@dimen/dimen_10x"
                     android:gravity="center_vertical"
                     android:text="业务单号:"
                     android:textColor="@color/back" />

                 <TextView
                     android:id="@+id/biznumbervalue"
                     android:textSize="@dimen/text_size_25"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:background="#FFFFFF"
                     android:ems="10"
                     android:gravity="center_vertical"
                     android:text="JN909098998"
                     android:textColor="@color/red" />
             </LinearLayout>

             <LinearLayout
                 android:layout_width="match_parent"
                 android:layout_height="@dimen/dimen_50y"
                 android:layout_marginBottom="@dimen/dimen_10y"
                 android:gravity="center_vertical"
                 android:orientation="horizontal" >

                 <TextView
                     android:id="@+id/boxs"
                     android:textSize="@dimen/text_size_35"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_marginLeft="@dimen/dimen_10x"
                     android:layout_marginRight="@dimen/dimen_10x"
                     android:gravity="center_vertical"
                     android:text="扫描钞箱数量:"
                     android:textColor="@color/back" />

                 <TextView
                     android:id="@+id/addstopnew_boxcount"
                     android:textSize="@dimen/text_size_40"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:background="#FFFFFF"
                     android:ems="10"
                     android:gravity="center"
                     android:text="0"
                     android:textColor="@color/red" />
             </LinearLayout>

             <LinearLayout
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content" >

             </LinearLayout>

             <Button
                android:id="@+id/outberforcheck"
                android:layout_marginLeft="@dimen/dimen_15x"
                android:visibility="visible"
                android:background="@drawable/buttom_selector_bg"
                android:textSize="@dimen/text_size_22"
                android:textColor="@color/white"
                android:layout_width="@dimen/dimen_230x"
                android:layout_height="@dimen/dimen_45y"
                android:text="钞箱出入库前检测" />

             <LinearLayout
                 android:layout_width="match_parent"
                 android:layout_height="@dimen/dimen_50y"
                 android:gravity="center_vertical"
                 android:orientation="horizontal" >

                 <TextView
                     android:id="@+id/TextView02"
                     android:textSize="@dimen/text_size_30"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_marginRight="@dimen/dimen_8x"
                     android:layout_weight="1"
                     android:gravity="center"
                     android:text="品牌信息"
                     android:textColor="@color/red" />

                 <TextView
                     android:id="@+id/TextView01"
                     android:textSize="@dimen/text_size_30"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_weight="1"
                     android:background="#FFFFFF"
                     android:ems="10"
                     android:gravity="center"
                     android:text="钞箱编号"
                     android:textColor="@color/red" />
             </LinearLayout>

             <LinearLayout
                 android:id="@+id/fragment_noway"
                 android:layout_width="match_parent"
                 android:layout_height="@dimen/dimen_280y"
                 android:gravity="center_vertical"
                 android:orientation="horizontal" >

                 <ListView
                     android:divider="@color/transparency"
                     android:dividerHeight="0px"
                     android:id="@+id/listview_noway"
                     android:layout_width="match_parent"
                     android:layout_height="match_parent"
                     android:layout_weight="1" >
                 </ListView>

             </LinearLayout>

         </LinearLayout>

    </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <Button
            android:enabled="false"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_30"
            android:background="@drawable/button_gray"
            android:layout_marginRight="@dimen/dimen_50x"
            android:id="@+id/noway_out"
            android:layout_width="@dimen/dimen_200x"
            android:layout_height="@dimen/dimen_70y"
            android:text="出库" />

        <Button
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_30"
            android:background="@drawable/buttom_selector_bg"
            android:id="@+id/cancel_do"
            android:layout_width="@dimen/dimen_200x"
            android:layout_height="@dimen/dimen_70y"
            android:text="取消" />

    </LinearLayout>

</LinearLayout>