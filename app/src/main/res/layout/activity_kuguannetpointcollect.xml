<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/bank_parent"
    android:layout_width="@dimen/dimen_480x"
    android:layout_height="@dimen/dimen_800y"
    android:background="#FFFFFF"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_120y"
        android:background="@color/blue_title"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="@dimen/dimen_80x"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/PDAsetting"
                android:layout_width="@dimen/dimen_40x"
                android:layout_height="@dimen/dimen_40y"
                android:src="@drawable/users" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dimen_280x"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView

                android:id="@+id/login_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_40y"
                android:text="网点人员验证"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_30" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dimen_100x"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_550y"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_40y">

            <LinearLayout
                android:layout_width="@dimen/dimen_180x"
                android:layout_height="@dimen/dimen_80y"
                android:background="@color/blue_title"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="请按入手指"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="@dimen/dimen_300x"
                android:layout_height="@dimen/dimen_80y"
                android:background="@color/blue_top"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/jj_resultmsg"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:text=""
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_400y"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="@dimen/dimen_240x"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_70y"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/username1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="网点人员"
                        android:textColor="@color/back"
                        android:textSize="@dimen/text_size_30" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_240y"
                    android:layout_marginBottom="@dimen/dimen_10y"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/jj_finger_leftcollect"
                        android:layout_width="@dimen/dimen_200x"
                        android:layout_height="@dimen/dimen_240y"
                        android:visibility="visible" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_70y"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/jj_login_name_left"
                        android:layout_width="@dimen/dimen_200x"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:background="@color/blue_text_bg"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_30" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="@dimen/dimen_240x"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_70y"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/username2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="网点人员"
                        android:textColor="@color/back"
                        android:textSize="@dimen/text_size_30" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_240y"
                    android:layout_marginBottom="@dimen/dimen_10y"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/jj_finger_right_collect"
                        android:layout_width="@dimen/dimen_200x"
                        android:layout_height="@dimen/dimen_240y"
                        android:visibility="visible" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_70y"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/jj_login_name_right_collect"
                        android:layout_width="@dimen/dimen_200x"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:background="@color/blue_text_bg"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_30" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/jj_show"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_70y"
            android:gravity="center"
            android:text="请第一位网点人员按压手指..."
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />

    </LinearLayout>
    <!--网点人员信息采集-->

</LinearLayout>