<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1y"
        android:background="#EFEEEE"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80y"
        android:layout_gravity="center"
        android:divider="@null"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/lv_checklibrarytaskno"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:text="physon"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_28" />

        <View
            android:layout_width="@dimen/dimen_1x"
            android:layout_height="match_parent"
            android:background="#EFEEEE"></View>

        <TextView
            android:id="@+id/lv_checklibrarytaskcounts"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="physon"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_28" />

        <View
            android:layout_width="@dimen/dimen_1x"
            android:layout_height="match_parent"
            android:background="#EFEEEE"></View>

        <TextView
            android:id="@+id/lv_checklibrarycreatetime"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:gravity="center"
            android:text="2"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_28" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1y"
        android:background="#EFEEEE"></View>
</LinearLayout>
