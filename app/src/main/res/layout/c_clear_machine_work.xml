<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dimen_480x"
    android:layout_height="@dimen/dimen_800y"
    android:background="#f6f6f6"
    android:orientation="vertical" >

    <LinearLayout
        android:gravity="center"
        android:background="@color/blue_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_123y" >

        <ImageView
            android:layout_marginRight="@dimen/dimen_15x"
            android:id="@+id/imageView1"
            android:layout_width="@dimen/dimen_49x"
            android:layout_height="@dimen/dimen_50y"
            android:src="@drawable/momey_img" />

        <TextView
           android:textSize="@dimen/text_size_40"
           android:textColor="@color/white"
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="清机加钞操作" />

        <ImageView
            android:layout_marginLeft="@dimen/dimen_80x"
            android:id="@+id/imageView2"
            android:layout_width="@dimen/dimen_64x"
            android:layout_height="@dimen/dimen_65y"
            android:src="@drawable/back_cirle" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="@dimen/dimen_450x"
            android:layout_height="@dimen/dimen_53y"
            android:layout_marginTop="@dimen/dimen_50y"
            android:gravity="center_vertical" >

            <TextView
                android:textSize="@dimen/text_size_30"
                android:textColor="@color/red"
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="清机加钞信息:" />

            <TextView
                 android:textSize="@dimen/text_size_30"
                android:textColor="@color/red"
                android:id="@+id/msg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dimen_450x"
            android:layout_height="@dimen/dimen_53y"
            android:layout_marginTop="@dimen/dimen_40y"
            android:background="@drawable/circle_edit_rudit"
            android:gravity="center" >

            <TextView
                android:id="@+id/textView3"
                android:textSize="@dimen/text_size_25"
                android:textColor="@color/gray"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ATM编号:" />

            <EditText
                android:id="@+id/editText1"
                android:textColor="@color/gray"
                android:layout_width="@dimen/dimen_320x"
                android:layout_height="@dimen/dimen_45y"
                android:background="#dde8f4"
                android:ems="10" >

                <requestFocus />
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dimen_450x"
            android:layout_height="@dimen/dimen_53y"
            android:layout_marginTop="@dimen/dimen_45y"
            android:background="@drawable/circle_edit_rudit"
            android:gravity="center" >

            <TextView
                android:id="@+id/textView4"
               android:textSize="@dimen/text_size_25"
                android:textColor="@color/gray"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="柜员编号:" />

            <EditText
                android:id="@+id/editText2"
                android:textColor="@color/gray"
                android:layout_width="@dimen/dimen_320x"
                android:layout_height="@dimen/dimen_45y"
                android:background="#dde8f4"
                android:ems="10" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dimen_450x"
            android:layout_height="@dimen/dimen_53y"
            android:layout_marginTop="@dimen/dimen_45y"
            android:background="@drawable/circle_edit_rudit"
            android:gravity="center" >

            <TextView
                android:layout_marginLeft="@dimen/dimen_10x"
                android:id="@+id/TextView02"
                android:textSize="@dimen/text_size_25"
                android:textColor="@color/gray"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ATM编号:" />

            <EditText
                android:id="@+id/EditText02"
                android:textColor="@color/gray"
                android:layout_width="@dimen/dimen_320x"
                android:layout_height="@dimen/dimen_45y"
                android:background="#dde8f4"
                android:ems="10" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dimen_450x"
            android:layout_height="@dimen/dimen_53y"
            android:layout_marginTop="@dimen/dimen_45y"
            android:background="@drawable/circle_edit_rudit"
            android:gravity="center" >

            <TextView
                android:layout_marginLeft="@dimen/dimen_10x"
                android:id="@+id/TextView01"
                android:textSize="@dimen/text_size_25"
                android:textColor="@color/gray"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="钞箱编号:" />

            <EditText
                android:id="@+id/EditText01"
                android:textColor="@color/gray"
                android:layout_width="@dimen/dimen_320x"
                android:layout_height="@dimen/dimen_45y"
                android:background="#dde8f4"
                android:ems="10" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_80y"
            android:layout_marginTop="@dimen/dimen_60y"
            android:gravity="center_horizontal" >

            <Button
                android:layout_marginRight="@dimen/dimen_30x"
                android:id="@+id/cleanmachine"
                android:textSize="@dimen/text_size_35"
                android:textColor="@color/white"
                android:layout_width="@dimen/dimen_200x"
                android:layout_height="@dimen/dimen_60y"
                android:background="@drawable/buttom_selector_bg"
                android:text="清机操作" />

            <Button
                android:id="@+id/addmoney"
                android:textSize="@dimen/text_size_35"
                android:textColor="@color/white"
                android:layout_width="@dimen/dimen_200x"
                android:layout_height="@dimen/dimen_60y"
                android:background="@drawable/buttom_selector_bg"
                android:text="加钞操作" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>