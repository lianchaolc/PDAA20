<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/black_boder"
    android:orientation="vertical">
    <TextView
        android:id="@+id/qf_zhouzhuan_saomiao_peisongdan"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60x"
        android:gravity="center"
        android:text=""
        android:textColor="@color/blue_top"
        android:textSize="@dimen/text_size_26" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/turnovercheckinfotvtitleonecvoun"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center|left"
            android:text="重控:"
            android:textColor="@color/blue_top"
            android:textSize="@dimen/text_size_26" />

        <TextView
            android:id="@+id/turnovercheckinfotvtitleinfoonecvoun"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:gravity="center"
            android:text="我是内容one"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_20" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/turnovercheckinfotvtitleonenumid"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center|left"
            android:text="现金:"
            android:textColor="@color/blue_top"
            android:textSize="@dimen/text_size_26" />

        <TextView
            android:id="@+id/turnovercheckinfotvtitleinfoonenumid"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:gravity="center"
            android:text=""
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_20" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/turnovercheckinfotvtitleoneordertype"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center|left"
            android:text="抵质押品:"
            android:textColor="@color/blue_top"
            android:textSize="@dimen/text_size_26" />

        <TextView
            android:id="@+id/turnovercheckinfotvtitleinfooneordertype"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:gravity="center"
            android:text="我是内容onerewqr qwrqwrqwdfnsdafijasfjpiosiofsoifosafjosdfiasoijfasoifoiasfjioasfijojiofjiofjiasiojf"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_20" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/turnovercheckinfotvtitleonepsnum"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center|left"
            android:text="周转箱："
            android:textColor="@color/blue_top"
            android:textSize="@dimen/text_size_26" />

        <TextView
            android:id="@+id/turnovercheckinfotvtitleinfoonepsnum"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:gravity="center"
            android:text=""
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_20" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="@dimen/dimen_10x"
        android:background="@color/back" />
</LinearLayout>
