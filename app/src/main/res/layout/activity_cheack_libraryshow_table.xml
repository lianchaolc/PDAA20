<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context="com.ljsw.pdachecklibrary.CheackLibraryshowTableActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80y"
        android:background="@color/blue_title"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/checklibrarybydizhi_imageView1"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_15x"
            android:src="@drawable/user_order" />

        <TextView
            android:id="@+id/check_librarytable_textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:text="盘库任务补扫"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_40"
            tools:ignore="all" />

        <Button
            android:id="@+id/check_librarytable_update"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_15x"
            android:layout_marginTop="@dimen/dimen_4y"
            android:background="@drawable/replace" />

        <ImageView
            android:id="@+id/check_librarytable_back"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:src="@drawable/back_cirle" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80y"
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/dimen_10x"
        android:layout_marginRight="@dimen/dimen_10x"
        android:orientation="horizontal">
    <TextView
        android:layout_width="@dimen/dimen_0x"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="操作人:"
        android:textColor="@color/back"
        android:textSize="@dimen/text_size_30" />

    <TextView
        android:id="@+id/tv_controllerchecklibryuser"
        android:layout_width="@dimen/dimen_0x"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="张某"
        android:textColor="@color/back"
        android:textSize="@dimen/text_size_30" />

</LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80y"
        android:layout_marginLeft="@dimen/dimen_10x"
        android:layout_marginRight="@dimen/dimen_10x"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <TextView
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="查库任务号:"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />

        <TextView
            android:id="@+id/cheachlibrary_taskno"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text=""
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />


    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/dimen_10x"
        android:layout_marginRight="@dimen/dimen_10x"
        android:orientation="horizontal">
        <TextView
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="缺少数量："
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />

        <TextView
            android:id="@+id/cheachlibrary_losscounts"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text=""
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />


    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/dimen_10x"
        android:layout_marginRight="@dimen/dimen_10x"
        android:orientation="horizontal">
        <TextView
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="日期："
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />

        <TextView
            android:id="@+id/cheachlibrary_creattime"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text=""
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/dimen_10x"
        android:layout_marginRight="@dimen/dimen_10x"
        android:orientation="horizontal">
        <TextView
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="柜子编号"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />

        <TextView
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="缺少数量"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />

    </LinearLayout>

    <ListView
        android:id="@+id/listview_showtable"
        android:layout_width="match_parent"
        android:layout_marginLeft="@dimen/dimen_10x"
        android:layout_marginRight="@dimen/dimen_10x"
        android:layout_height="262dp"></ListView>
</LinearLayout>
