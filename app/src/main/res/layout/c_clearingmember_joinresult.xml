<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dimen_480x"
    android:layout_height="@dimen/dimen_800y"
    android:orientation="vertical"
    android:background="#FFFFFF" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_120y"
        android:background="@color/blue_title"
        android:orientation="horizontal" >

        <LinearLayout
            android:paddingLeft="25px"
            android:layout_marginRight="@dimen/dimen_10x"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center" >

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="@dimen/dimen_40x"
                android:layout_height="@dimen/dimen_40y"
                android:src="@drawable/users" />
            
        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dimen_280x"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <TextView
                android:textSize="@dimen/text_size_35"
                android:textColor="@color/white"
                android:id="@+id/user_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="清分员交接"
                android:layout_marginTop="@dimen/dimen_35y" />

        </LinearLayout>
        
         <LinearLayout
            android:layout_width="@dimen/dimen_100x"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center" >

             <ImageView
                 android:id="@+id/joinrs_back"
                 android:layout_width="@dimen/dimen_70x"
                 android:layout_height="@dimen/dimen_70y"
                 android:src="@drawable/back_cirle" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_500y"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="@dimen/dimen_400x"
            android:layout_height="@dimen/dimen_200y"
            android:background="@color/bleu_pressdown"
            android:gravity="center"
            android:padding="10px" >

            <ImageView
                android:id="@+id/result_img"
                android:layout_width="@dimen/dimen_50x"
                android:layout_height="@dimen/dimen_50y"
                android:layout_marginRight="@dimen/dimen_20x"
                android:src="@drawable/sccuss" />

            <TextView
                android:id="@+id/result_text"
                android:textSize="@dimen/text_size_30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="空钞箱交接完成"
                android:textColor="@color/red" />
        </LinearLayout>

        
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_100y"
        android:orientation="horizontal"
        android:gravity="center" >

        <Button
            android:background="@drawable/buttom_selector_bg"
            android:textSize="@dimen/text_size_35"
            android:textColor="@color/white"
            android:id="@+id/sure"
            android:layout_width="@dimen/dimen_250x"
            android:layout_height="@dimen/dimen_60y"
            android:text="确定" />

        </LinearLayout>

   

</LinearLayout>