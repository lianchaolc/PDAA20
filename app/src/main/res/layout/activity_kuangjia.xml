<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80y"
        android:background="@color/blue_title"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_15x"
            android:src="@drawable/user_order" />

        <TextView
            android:id="@+id/dz_frametextView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:layout_toRightOf="@id/imageView1"
            android:text="档案柜"
            android:textColor="@color/white"
            android:textSize="@dimen/title_text_size" />

        <Button
            android:id="@+id/ql_ruku_update"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginRight="@dimen/dimen_10x"
            android:layout_marginTop="@dimen/dimen_4y"
            android:layout_toLeftOf="@id/ql_ruku_back"
            android:background="@drawable/replace" />

        <ImageView
            android:id="@+id/ql_ruku_back"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dimen_15x"
            android:src="@drawable/back_cirle" />
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <RelativeLayout
                android:id="@+id/zhuangdai"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_80y"
                android:layout_marginTop="@dimen/dimen_10y"
                android:background="@color/bule_main">

                <ImageView
                    android:id="@+id/updatepass"
                    android:layout_width="@dimen/dimen_60x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_30x"
                    android:src="@drawable/user_order2"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_11x"
                    android:layout_toRightOf="@+id/updatepass"
                    android:text="抵制押品装袋"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/resistpledgebagging_tv"
                    android:layout_width="@dimen/dimen_40x"
                    android:layout_height="@dimen/dimen_40y"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20x"
                    android:layout_marginRight="@dimen/dimen_30x"
                    android:layout_toRightOf="@+id/textView1"
                    android:background="@drawable/business_number"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_25" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/outroom"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_80y"
                android:layout_marginTop="@dimen/dimen_10y"
                android:background="@color/bule_main">

                <ImageView
                    android:id="@+id/updatepass"
                    android:layout_width="@dimen/dimen_60x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_30x"
                    android:src="@drawable/user_order2"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_11x"
                    android:layout_toRightOf="@+id/updatepass"
                    android:text="抵质押品出库"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/resistthecollateral"
                    android:layout_width="@dimen/dimen_40x"
                    android:layout_height="@dimen/dimen_40y"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20x"
                    android:layout_marginRight="@dimen/dimen_30x"
                    android:layout_toRightOf="@+id/textView1"
                    android:background="@drawable/business_number"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_25" />
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/ruku"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_80y"
                android:layout_marginTop="@dimen/dimen_10y"
                android:background="@color/bule_main">

                <ImageView
                    android:id="@+id/updatepass"
                    android:layout_width="@dimen/dimen_60x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_30x"
                    android:src="@drawable/user_order2"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/resistcollateralinhoustv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_11x"
                    android:layout_toRightOf="@+id/updatepass"
                    android:text="抵质押品入库"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30" />

                <TextView
                    android:id="@+id/resistcollateralinhous"
                    android:layout_width="@dimen/dimen_40x"
                    android:layout_height="@dimen/dimen_40y"
                    android:layout_alignBaseline="@+id/resistcollateralinhoustv"
                    android:layout_alignBottom="@+id/resistcollateralinhoustv"
                    android:layout_marginLeft="@dimen/dimen_10x"
                    android:layout_toRightOf="@+id/resistcollateralinhoustv"
                    android:background="@drawable/business_number"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_25" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/zhuangxiang"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_80y"
                android:layout_marginTop="@dimen/dimen_10y"
                android:background="@color/bule_main">

                <ImageView
                    android:id="@+id/updatepass"
                    android:layout_width="@dimen/dimen_60x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_30x"
                    android:src="@drawable/user_order2"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_11x"
                    android:layout_toRightOf="@+id/updatepass"
                    android:text="抵制押品装箱"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/resistcollateralpacking"
                    android:layout_width="@dimen/dimen_40x"
                    android:layout_height="@dimen/dimen_40y"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20x"
                    android:layout_marginRight="@dimen/dimen_30x"
                    android:layout_toRightOf="@+id/textView1"
                    android:background="@drawable/business_number"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_25" />
            </RelativeLayout>
            <!-- 账户资料装袋 -->

            <RelativeLayout
                android:id="@+id/zhanghuziliaozhuangdai"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_80y"
                android:layout_marginTop="@dimen/dimen_10y"
                android:background="@color/bule_main"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/updatepass"
                    android:layout_width="@dimen/dimen_60x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_30x"
                    android:src="@drawable/user_order2"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_11x"
                    android:layout_toRightOf="@+id/updatepass"
                    android:text="账户资料装袋"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/accountinfotiontobagging"
                    android:layout_width="@dimen/dimen_40x"
                    android:layout_height="@dimen/dimen_40y"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20x"
                    android:layout_marginRight="@dimen/dimen_30x"
                    android:layout_toRightOf="@+id/textView1"
                    android:background="@drawable/business_number"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_25" />
            </RelativeLayout>
            <!-- 账户资料入库 -->

            <RelativeLayout
                android:id="@+id/zhanghuziliaoruku"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_80y"
                android:layout_marginTop="@dimen/dimen_10y"
                android:background="@color/bule_main">

                <ImageView
                    android:id="@+id/updatepass"
                    android:layout_width="@dimen/dimen_60x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_30x"
                    android:src="@drawable/user_order2"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_11x"
                    android:layout_toRightOf="@+id/updatepass"
                    android:text="账户资料入库"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/accountintionminhome_number_tv"
                    android:layout_width="@dimen/dimen_40x"
                    android:layout_height="@dimen/dimen_40y"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20x"
                    android:layout_marginRight="@dimen/dimen_30x"
                    android:layout_toRightOf="@+id/textView1"
                    android:background="@drawable/business_number"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_25" />
            </RelativeLayout>
            <!-- 账户资料出库借阅 -->

            <RelativeLayout
                android:id="@+id/zhanghuziliaochukujieyue"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_80y"
                android:layout_marginTop="@dimen/dimen_10y"
                android:background="@color/bule_main">

                <ImageView
                    android:id="@+id/updatepass"
                    android:layout_width="@dimen/dimen_60x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_30x"
                    android:src="@drawable/user_order2"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_11x"
                    android:layout_toRightOf="@+id/updatepass"
                    android:text="账户资料出库借阅"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/accountintionoutboundborrowing"
                    android:layout_width="@dimen/dimen_40x"
                    android:layout_height="@dimen/dimen_40y"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20x"
                    android:layout_marginRight="@dimen/dimen_30x"
                    android:layout_toRightOf="@+id/textView1"
                    android:background="@drawable/business_number"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_25" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/zhanghuziliaoguihuan"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_80y"
                android:layout_marginTop="@dimen/dimen_10y"
                android:background="@color/bule_main">

                <ImageView
                    android:id="@+id/updatepass"
                    android:layout_width="@dimen/dimen_60x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_30x"
                    android:src="@drawable/user_order2"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_11x"
                    android:layout_toRightOf="@+id/updatepass"
                    android:text="账户资料归还"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/returnofaccountinformation_tv"
                    android:layout_width="@dimen/dimen_40x"
                    android:layout_height="@dimen/dimen_40y"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20x"
                    android:layout_marginRight="@dimen/dimen_30x"
                    android:layout_toRightOf="@+id/textView1"
                    android:background="@drawable/business_number"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_25" />
            </RelativeLayout>
            <!--账户资料归还入库待入库 -->
            <RelativeLayout
                android:id="@+id/zhanghuziliaoguihuanrukudairuku"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_80y"
                android:layout_marginTop="@dimen/dimen_10y"

                android:background="@color/bule_main">

                <ImageView
                    android:id="@+id/updatepass"
                    android:layout_width="@dimen/dimen_60x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_30x"
                    android:src="@drawable/user_order2"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_11x"
                    android:layout_toRightOf="@+id/updatepass"
                    android:text="归还入库待确认列表"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/returntothelistofmaterialstobereturned_tv"
                    android:layout_width="@dimen/dimen_40x"
                    android:layout_height="@dimen/dimen_40y"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20x"
                    android:layout_marginRight="@dimen/dimen_30x"
                    android:layout_toRightOf="@+id/textView1"
                    android:background="@drawable/business_number"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_25" />
            </RelativeLayout>
            <!-- 账户中心入账中心 -->
            <RelativeLayout
                android:id="@+id/ruzhanghuzhongxin"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_80y"
                android:layout_marginTop="@dimen/dimen_10y"
                android:background="@color/bule_main">

                <ImageView
                    android:id="@+id/updatepass"
                    android:layout_width="@dimen/dimen_60x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_30x"
                    android:src="@drawable/user_order2"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_11x"
                    android:layout_toRightOf="@+id/updatepass"
                    android:text="入账户中心"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_30"
                    tools:ignore="all"/>

                <TextView
                    android:id="@+id/account_center_wangdian"
                    android:layout_width="@dimen/dimen_40x"
                    android:layout_height="@dimen/dimen_40y"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20x"
                    android:layout_marginRight="@dimen/dimen_30x"
                    android:layout_toRightOf="@+id/textView1"
                    android:background="@drawable/business_number"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_25" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>