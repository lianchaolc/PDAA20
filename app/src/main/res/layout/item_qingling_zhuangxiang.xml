<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dimen_400x"
    android:layout_height="@dimen/dimen_50y"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/tv_item_qingling_zhuangxiang_type"
        android:layout_width="@dimen/dimen_0x"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_weight="2.8"
        android:textSize="@dimen/text_size_25"
        android:textColor="@color/back"
        android:text="类型" />

    <TextView
        android:id="@+id/tv_item_qingling_zhuangxiang_count"
        android:layout_width="@dimen/dimen_0x"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_weight="2.9"
        android:textSize="@dimen/text_size_25"
        android:textColor="@color/back"
        android:text="" />

    <TextView
        android:id="@+id/tv_item_qingling_zhuangxiang_delete"
        android:layout_width="@dimen/dimen_0x"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="删除"
        android:layout_weight="1"
        android:textColor="@color/red"
        android:textSize="@dimen/text_size_30" />

</LinearLayout>