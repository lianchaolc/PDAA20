<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFEEEE"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80y"
        android:background="@color/blue_title"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:text="账户资料出库交接"
            android:textColor="@color/white"
            android:textSize="@dimen/title_text_size" />


        <ImageView
            android:id="@+id/ql_ruku_back"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_60x"
            android:layout_marginRight="@dimen/dimen_10x"
            android:src="@drawable/back_cirle" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:layout_gravity="center"
        android:background="@color/white"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_60y"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/dimen_8x"
            android:layout_marginRight="@dimen/dimen_8x"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="操作人:"
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28" />

            <TextView
                android:id="@+id/accountintionouthous_orerater"
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text=""
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/dimen_8x"
            android:layout_marginRight="@dimen/dimen_8x"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="操作人:"
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28" />

            <TextView
                android:id="@+id/accountdatadeliverycheckup_user"
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="李三样"
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dimen_10y"
        android:layout_marginTop="@dimen/dimen_10y"
        android:background="@color/white"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_80y"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:layout_marginRight="@dimen/dimen_10x"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="出库任务编号:"
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28" />

            <TextView
                android:id="@+id/accountdatadeliverycheckup_tasknumber_tv"
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text=""
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1y"
            android:background="#E6E5E5" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_60y"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:layout_marginRight="@dimen/dimen_10x"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="出库任务类型:"
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28" />

            <TextView
                android:id="@+id/accountdatadeliverycheckup_type_tv"
                android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/dimen_10x"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text=""
                android:textColor="@color/back"
                android:textSize="@dimen/text_size_28" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1y"
            android:background="#E6E5E5" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_60y"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="@dimen/dimen_0x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="@dimen/dimen_0x"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dimen_10x"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="待扫描:"
                        android:textColor="@color/back"
                        android:textSize="@dimen/text_size_28" />

                    <TextView
                        android:id="@+id/accountdatadeliverycheckup_unscan_tv"
                        android:layout_width="@dimen/dimen_0x"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center|left"
                        android:text="4"
                        android:textColor="@color/back"
                        android:textSize="@dimen/text_size_28" >
                    </TextView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="@dimen/dimen_0x"
                    android:layout_height="@dimen/dimen_60y"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="@dimen/dimen_0x"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="已扫描 :"
                        android:textColor="@color/back"
                        android:textSize="@dimen/text_size_28" />

                    <TextView
                        android:id="@+id/accountdatadeliverycheckup_scan_tv"
                        android:layout_width="@dimen/dimen_0x"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center|left"
                        android:text="1"
                        android:textColor="@color/back"
                        android:textSize="@dimen/text_size_28" >
                    </TextView>
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_1y"
                android:background="#E6E5E5" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_300y"
                android:layout_marginTop="@dimen/dimen_10y"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <ListView
                    android:id="@+id/accountdatadeliverycheckup_unscanlistview"
                    android:layout_width="@dimen/dimen_0x"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/dimen_8x"
                    android:layout_marginRight="@dimen/dimen_8x"
                    android:layout_weight="1"
                    android:divider="#00000000" >
                </ListView>

                <View
                    android:layout_width="@dimen/dimen_1x"
                    android:layout_height="match_parent"
                    android:background="#E6E5E5" />

                <ListView
                    android:id="@+id/accountdatadeliverycheckup_scanlistview"
                    android:layout_width="@dimen/dimen_0x"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/dimen_8x"
                    android:layout_marginRight="@dimen/dimen_8x"
                    android:layout_weight="1"
                    android:divider="#00000000" >
                </ListView>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:layout_marginLeft="@dimen/dimen_10x"
        android:layout_marginRight="@dimen/dimen_10x"
        android:layout_marginTop="@dimen/dimen_5y"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/accountdatadeliverycheckup_surebtn"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_weight="1"
            android:background="@drawable/buttom_selector_bg"
            android:gravity="center"
            android:text="交接"
            android:textSize="@dimen/text_size_30" />

        <Button
            android:id="@+id/accountdatadeliverycheckup_cancle_btn"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_weight="1"
            android:background="@drawable/buttom_selector_bg"
            android:gravity="center"
            android:text="取消"
            android:textSize="@dimen/text_size_30" />
    </LinearLayout>

</LinearLayout>