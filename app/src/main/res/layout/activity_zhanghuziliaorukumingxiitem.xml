<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#F1f1f1"
    tools:context="com.ljsw.tjbankpad.baggingin.activity.ZhangHuZLaActivity" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80y"
        android:background="@color/blue_title"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_15x"
            android:src="@drawable/user_order" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:text="账户资料入库明细"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_50" />

        <Button
            android:id="@+id/ql_ruku_update"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_15x"
            android:layout_marginTop="@dimen/dimen_4y"
            android:background="@drawable/replace" />

        <ImageView
            android:id="@+id/ql_ruku_back"
            android:layout_width="@dimen/dimen_60x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:src="@drawable/back_cirle" />
    </LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:background="@color/white"
     
         android:orientation="vertical" >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/dzyp_"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center|left"
            android:text="操作人员:"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />

        <TextView
            android:id="@+id/zhzichukumingxi_username"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="@dimen/dimen_30y"
            android:layout_weight="1"
            android:gravity="center|left"
            android:text="xxx"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" >
        </TextView>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1y"
        android:background="#E6E5E5" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="@dimen/dimen_30y"
            android:layout_weight="1"
            android:gravity="center"
            android:text="出库任务编号:"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />
        <TextView
            android:id="@+id/dzyp_number"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="@dimen/dimen_30y"
            android:layout_weight="1"
            android:gravity="center"
            android:text="RW0000002"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" />

      
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1y"
        android:background="#E6E5E5" />

 

<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
             android:id="@+id/dzyp_"
             android:layout_width="@dimen/dimen_0x"
             android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="账户资料袋数量:"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30"
            tools:ignore="all"
            />

        <TextView
            android:id="@+id/zhanghuziliaoruku"
            android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center|left"
            android:text=""
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" >
        </TextView>
</LinearLayout>
 <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1y"
        android:background="#E6E5E5" />

    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/dimen_10x"
        android:layout_marginRight="@dimen/dimen_10x"
        android:orientation="horizontal" >


        <TextView
            android:id="@+id/zhzlrkmxiid"
              android:layout_width="@dimen/dimen_0x"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center|left"
            android:text="封包编号:"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" >
        </TextView>
        
         <TextView
            android:id="@+id/zhzlrkmxihuowei"
            android:layout_width="@dimen/dimen_0x"
                android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="待放入货位"
            android:textColor="@color/back"
            android:textSize="@dimen/text_size_30" >
        </TextView>
</LinearLayout>


        <ListView
            android:layout_marginTop="@dimen/dimen_15y"
            android:id="@+id/hzlrkmxilistview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_135y"
            android:layout_marginLeft="@dimen/dimen_8x"
            android:layout_marginRight="@dimen/dimen_8x" >
        </ListView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_60y"
        android:layout_marginRight="@dimen/dimen_10x"
        android:layout_marginTop="@dimen/dimen_20y"
        android:gravity="center"
        android:orientation="horizontal" >

<Button
            android:id="@+id/zhanghuiziliaomingxisaomiaobtn"
            android:layout_width="@dimen/dimen_200x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:layout_marginRight="@dimen/dimen_10x"
            android:text="扫描"
            android:gravity="center"
            android:background="@drawable/buttom_selector_bg"
            android:textSize="@dimen/text_size_30" />
        <Button
            android:id="@+id/zhzirkmingxibtn"
            android:layout_width="@dimen/dimen_200x"
            android:layout_height="@dimen/dimen_60y"
            android:layout_marginLeft="@dimen/dimen_10x"
            android:layout_marginRight="@dimen/dimen_10x"
            android:text="出库交接"
            android:gravity="center"
            android:background="@drawable/buttom_selector_bg"
            android:textSize="@dimen/text_size_30" />

     </LinearLayout>
    </LinearLayout>
</LinearLayout>